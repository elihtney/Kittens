<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/tablethomenobutton"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.brandon.kittenfactory.HomeActivity">

    <Button
        android:layout_width="510dp"
        android:layout_height="100dp"
        android:id="@+id/start_btn"
        android:background="@drawable/starthome"
        android:layout_marginBottom="43dp"
        android:layout_above="@+id/instruction_btn"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="510dp"
        android:layout_height="100dp"
        android:id="@+id/instruction_btn"
        android:background="@drawable/instructionhome"
        android:layout_marginBottom="83dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/start_btn"
        android:layout_alignStart="@+id/start_btn" />

</RelativeLayout>
